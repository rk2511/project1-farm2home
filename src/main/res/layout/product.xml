<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.vikki.farm.Product">

    <ImageButton
        android:id="@+id/imageButton18"
        android:layout_width="250dp"
        android:layout_height="200dp"
        android:layout_marginTop="53dp"
        app:srcCompat="@drawable/milk3"
        android:layout_alignParentTop="true"
        android:onClick="aboutMilk"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/button12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/button13"
        android:layout_below="@+id/imageButton18"
        android:layout_marginTop="99dp"
        android:text="Coming soon" />

    <Button
        android:id="@+id/button13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/imageButton18"
        android:layout_below="@+id/button12"
        android:layout_marginEnd="52dp"
        android:layout_marginTop="69dp"

        android:text="Coming Soon" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button12"
        android:layout_below="@+id/imageButton18"
        android:layout_marginEnd="26dp"
        android:layout_marginTop="49dp"
        android:text="Milk" />
</RelativeLayout>